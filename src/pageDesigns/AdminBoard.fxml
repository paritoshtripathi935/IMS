                <Pane layoutX="23.0" layoutY="-37.0" prefHeight="175.0" prefWidth="167.0" styleClass="logo" stylesheets="@../css/app.css" />
                <JFXButton fx:id="IncomBtn" alignment="CENTER" buttonType="RAISED" layoutX="28.0" layoutY="116.0" onAction="#IncomingOpenAction" prefHeight="40.0" prefWidth="125.0" styleClass="dash-btn" text="INCOMING" textAlignment="CENTER" textFill="WHITE">
                    <font>
                        <Font name="Segoe UI" size="13.0" />
                    </font>
                </JFXButton>
                <JFXButton fx:id="OutgoingBtn" alignment="CENTER" buttonType="RAISED" layoutX="28.0" layoutY="175.0" onAction="#OutgoingOpenAction" prefHeight="40.0" prefWidth="125.0" styleClass="dash-btn" text="OUTGOING" textFill="WHITE">
                    <font>
                        <Font name="Segoe UI" size="13.0" />
                    </font></JFXButton>
                <JFXButton fx:id="UMSbutton" alignment="CENTER" buttonType="RAISED" layoutX="20.0" layoutY="235.0" onAction="#UMSOpenAction" prefHeight="40.0" prefWidth="134.0" styleClass="dash-btn" text="UMS" textFill="WHITE">
                    <font>
                        <Font name="Segoe UI" size="13.0" />
                    </font></JFXButton>
                <JFXButton alignment="CENTER" buttonType="RAISED" layoutX="22.0" layoutY="296.0" prefHeight="40.0" prefWidth="132.0" styleClass="dash-btn" text="INVENTORY" textFill="WHITE">
                    <font>
                        <Font name="Segoe UI" size="13.0" />
                    </font></JFXButton>
                <JFXButton alignment="CENTER" buttonType="RAISED" layoutX="15.0" layoutY="357.0" prefHeight="40.0" prefWidth="140.0" styleClass="dash-btn" text="REPORTS" textFill="WHITE">
                    <font>
                        <Font name="Segoe UI" size="13.0" />
                    </font></JFXButton>
                <JFXButton alignment="CENTER" buttonType="RAISED" layoutX="23.0" layoutY="543.0" prefHeight="30.0" prefWidth="97.0" styleClass="btn-log" text="LOGOUT" textFill="WHITE">
                    <font>
                        <Font name="Segoe UI" size="13.0" />
                    </font></JFXButton>
            </children>
        </Pane>
        <Pane layoutX="159.0" prefHeight="28.0" prefWidth="841.0" styleClass="dash-title">
            <children>
                <Text fill="WHITE" layoutX="14.0" layoutY="20.0" strokeType="OUTSIDE" strokeWidth="0.0" text="DASHBOARD">
                    <font>
                        <Font name="System Bold" size="15.0" />
                    </font>
                </Text>
            </children></Pane>
        <Pane layoutX="174.0" layoutY="48.0" prefHeight="232.0" prefWidth="484.0" styleClass="dash-tile" />
        <Pane layoutX="676.0" layoutY="49.0" prefHeight="232.0" prefWidth="311.0" styleClass="dash-tile" />
        <Pane layoutX="460.0" layoutY="305.0" prefHeight="232.0" prefWidth="527.0" styleClass="dash-tile">
            <children>
                <LineChart fx:id="lineChart" prefHeight="232.0" prefWidth="527.0" title="Inventory Storage ">
                    <xAxis>
                        <CategoryAxis side="BOTTOM" />
                    </xAxis>
                    <yAxis>
                        <NumberAxis side="LEFT" />
                    </yAxis>
                </LineChart>
                <Button layoutX="416.0" layoutY="7.0" mnemonicParsing="false" onAction="#loadChart" text="Show Chart" textFill="#1b1313">
                    <font>
                        <Font name="Segoe UI" size="13.0" />
                    </font>
                    <effect>
                        <Lighting>
                            <bumpInput>
                                <Shadow />
                            </bumpInput>
                            <light>
                                <Light.Distant />
                            </light>
                        </Lighting>
                    </effect>
                </Button>
            </children></Pane>
        <Pane layoutX="173.0" layoutY="304.0" prefHeight="232.0" prefWidth="268.0" styleClass="dash-tile" />
